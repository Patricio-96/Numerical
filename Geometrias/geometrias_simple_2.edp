border a(t=0, 2*pi) {x=cos(t); y=sin(t); label=1;}; //mesh Th = buildmesh(a(50)); 
border b(t=0, 2*pi) {x=0.2*cos(t)+0.3; y=0.3*sin(t)+0.3; label=1;}; 
border c(t=0, 2*pi) {x=0.2*cos(t)-0.3; y=0.3*sin(t)+0.3; label=1;};
border d(t=0,2*pi){ x=0.1*cos(t); y=0.1*sin(t);label=1;} 
border e(t=pi,2*pi){ x=0.3*cos(t); y=0.1*sin(t)-0.35;label=1;} 
mesh Th = buildmesh(a(50)+b(-50)+c(-50)+d(-50)+e(-50)); 
plot(Th); 